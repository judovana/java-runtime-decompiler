<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- This usage of static field is intentional, and most importantly, it passes in -Plegacy (jdk8 bytecode), but fails in newer -->
         <Match>
            <Method name="~.*SetLast.*" />
            <Bug pattern="ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD" />
        </Match>
    <!-- May expose internal representation by returning reference to mutable object -->
        <!-- Swing components -->
        <Match>
            <Method name="~.*get.*Frame.*" />
            <Bug pattern="EI_EXPOSE_REP" />
        </Match>
        <Match>
            <Method name="~.*get.*Panel.*" />
            <Bug pattern="EI_EXPOSE_REP" />
        </Match>
        <Match>
            <Method name="~.*get.*Button.*" />
            <Bug pattern="EI_EXPOSE_REP" />
        </Match>
        <Match>
            <Method name="~.*get.*TextField.*" />
            <Bug pattern="EI_EXPOSE_REP" />
        </Match>
        <Match>
            <Method name="~.*get.*JList.*" />
            <Bug pattern="EI_EXPOSE_REP" />
        </Match>
        <Match>
            <Method name="~.*get.*Url.*" />
            <Bug pattern="EI_EXPOSE_REP" />
        </Match>

        <!-- Objects -->
        <Match>
            <Method name="~.*get.*Method.*" />
            <Bug pattern="EI_EXPOSE_REP" />
        </Match>
        <Match>
            <Method name="~.*get.*Manager.*" />
            <Bug pattern="EI_EXPOSE_REP" />
        </Match>
        <Match>
            <Method name="~.*get.*URL.*" />
            <Bug pattern="EI_EXPOSE_REP" />
        </Match>
        <Match>
            <Method name="~.*getVmDecompilerStatus.*" />
            <Bug pattern="EI_EXPOSE_REP" />
        </Match>

    <!-- May expose internal representation by incorporating reference to mutable object -->
        <!-- Constructors -->
        <Match>
            <Method name="&lt;init&gt;"/>
            <Bug pattern="EI_EXPOSE_REP2" />
        </Match>

        <!-- Objects-->
        <Match>
            <Method name="~.*set.*Method.*" />
            <Bug pattern="EI_EXPOSE_REP2" />
        </Match>
        <Match>
            <Method name="~.*setVmDecompilerStatus.*" />
            <Bug pattern="EI_EXPOSE_REP2" />
        </Match>
</FindBugsFilter>
